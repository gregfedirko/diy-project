<div class="container-fluid">
  <div class="page-header">
    <h1>Featured</h1>
  </div>
  <div class="row">
  {{#each featured}}
    <div class="col-sm-6 col-md-6">
      <div class="thumbnail">
        <div class="caption">
          <h3>{{title}}</h3>
          <p><a href="maker/{{maker.url}}">{{maker.nickname}}</a></p>
        </div>
      </div>
    </div>
  {{/each}}
  </div>
</div>


